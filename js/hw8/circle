class Circle {
  constructor() {
    this.radius = radius;
  }
  get radius() {
    return this.radius;
  }
  set radius(radius) {
    this.radius = radius;
  }
  get diameter() {
    return this.radius * 2;
  }
  areaOfCircle() {
    areaOfCircle = Math.PI * this.radius * this.radius;
  }
  lengthOgCircle() {
    lengthOgCircle = 2 * Math.PI * this.radius;
  }
}
const c = new Circle
